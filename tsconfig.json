{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true
  },
  "baseUrl": ".",
  "paths": {
    "@/*": ["src/*"],
    "@shared-components/*": ["shared/components/*"],
    "@shared-constants/*": ["shared/constant/*"],
    "@pages/*": ["pages/*"],
    "@lib/*": ["lib/*"],
    "@themes": ["shared/constant/theme/index"],
    "@assets": ["assets/*"],
    "@features": ["features/*"],
    "@services": ["services/*"],
    "@types/*": ["@types/*"]
  }
}
